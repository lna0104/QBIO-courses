unknown <- function(x) {
len <- length(x)
i <- 1
while((i < len) && (x[i] < x[i + 1])) {
i <- i + 1
}
return(x[1:i])
}
unknown(c(4, 9, 13, 5, 6, 10, 1))
x <- 5
if (((x < 0) || (x > 10)) && (x != 17)) {
stop("Number is out of range!")
} else {
print(paste("Great,", x, "it is!"))
}
